{
  "enabledReports": ["ThermalStatus", "FanInventory"],
  "selections": [
    {
      "from": "ThermalStatus",
      "metrics": [
        "Reading",
        "Health",
        "UpperCriticalThreshold",
        "UpperNonCriticalThreshold",
        "LowerCriticalThreshold",
        "LowerNonCriticalThreshold"
      ]
    },
    {
      "from": "FanInventory",
      "metrics": ["CurrentReading"]
    }
  ],
  "sub_method": "sub",
  "host": "localhost"
}
