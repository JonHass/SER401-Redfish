{
  "RedFishData": {
    "path": "../../DCSimTools/Redfish-Mockup-Server/redfish"
  },
  "MockupData": {
    "MockupPatterns": [
      {
		"enabled": true,
        "name": "Sawtooth CPU(1) Percent",
        "path": "/v1/TelemetryService/MetricReportDefinitions/CPUSensor/",
        "timedelay": 3,
        "pattern": "sawtooth",
        "MetricValueTemplate": {
          "MemberID": "Temperature",
          "MetricValue": "#value",
          "TimeStamp": "#timestamp",
          "MetricProperty": "/redfish/v1/Chassis/System.Embedded.1/CPU/{PWild}/CPUSensor/Temperature"
        }
      },
      {
	    "enabled": true,
        "name": "Pingpong CPU(2) Percent",
        "path": "/v1/TelemetryService/MetricReportDefinitions/CPUSensor/",
        "timedelay": 5,
        "pattern": "fullrand",
        "min": 70,
        "max": 80,
        "step": 1,
        "MetricValueTemplate": {
          "MemberID": "PowerConsumption",
          "MetricValue": "#value",
          "TimeStamp": "#timestamp",
          "MetricProperty": "/redfish/v1/Chassis/System.Embedded.1/CPU/{PWild}/CPUSensor/PowerConsumption"
        }
      },
      {
	    "enabled": true,
        "name": "Random CPU(3) Percent",
        "path": "/v1/TelemetryService/MetricReportDefinitions/CPUSensor/",
        "timedelay": 10,
        "pattern": "fullrand",
        "min": 45,
        "max": 90,
        "MetricValueTemplate": {
          "MemberID": "MinTemperature",
          "MetricValue": "#value",
          "TimeStamp": "#timestamp",
          "MetricProperty": "/redfish/v1/Chassis/System.Embedded.1/CPU/{PWild}/CPUSensor/MinTemperature"
        }
      },
      {
	    "enabled": true,
        "name": "Rubberband Memory Util Tray 2",
        "path": "/v1/TelemetryService/MetricReportDefinitions/CPUSensor/",
        "timedelay": 2,
        "pattern": "rubberband",
        "center": 70,
        "step": 5,
        "MetricValueTemplate": {
          "MemberID": "MaxTemperature",
          "MetricValue": "#value",
          "TimeStamp": "#timestamp",
          "MetricProperty": "/redfish/v1/Chassis/System.Embedded.1/CPU/{PWild}/CPUSensor/MaxTemperature"
        }
      }
    ]
  }
}
